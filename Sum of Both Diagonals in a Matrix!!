#include <stdio.h>

int main() {
    int N;
    int A[100][100];
    long long primarySum = 0, secondarySum = 0, totalSum;
    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            scanf("%d", &A[i][j]);
        }
    }
    for (int i = 0; i < N; i++) {
        primarySum += A[i][i];
        secondarySum += A[i][N - i - 1];
    }
    totalSum = primarySum + secondarySum;
    if (N % 2 == 1) {
        totalSum -= A[N / 2][N / 2];
    }
    printf("Sum of Primary Diagonal: %lld\n", primarySum);
    printf("Sum of Secondary Diagonal: %lld\n", secondarySum);
    printf("Total Sum of Both Diagonals: %lld\n", totalSum);

    return 0;
}
