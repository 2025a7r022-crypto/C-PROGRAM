#include <stdio.h>

int main() {
    int marks[5];
    double total = 0, percentage;
    char grade;

    // Input marks for 5 subjects
    for(int i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
        total += marks[i];
    }

    // Calculate percentage
    percentage = total / 5.0;

    // Determine grade
    if (percentage >= 90) {
        grade = 'A';
    } else if (percentage >= 80) {
        grade = 'B';
    } else if (percentage >= 70) {
        grade = 'C';
    } else if (percentage >= 60) {
        grade = 'D';
    } else if (percentage >= 40) {
        grade = 'E';
    } else {
        grade = 'F';
    }

    // Print result
    printf("Percentage = %.2lf\n", percentage);
    printf("Grade = %c\n", grade);

return 0;
}
