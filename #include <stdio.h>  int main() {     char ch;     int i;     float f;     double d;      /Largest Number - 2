#include <stdio.h>

int main() {
    int A, B, C;

    // Input three numbers
    scanf("%d %d %d", &A, &B, &C);

    // Find largest
    if (A >= B && A >= C) {
        printf("%d\n", A);
    } else if (B >= A && B >= C) {
        printf("%d\n", B);
    } else {
        printf("%d\n", C);
    }

    return 0;
}
