#include <stdio.h>

int main() {
    char ch;
    int i;
    float f;
    double d;

    // Read inputs: character, integer, float, and double
    scanf(" %c %d %f %lf", &ch, &i, &f, &d);

    // Print the values with the required formatting
    printf("%c %d %.2f %.6lf\n", ch, i, f, d);

    return 0;
}
